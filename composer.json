{
    "name": "wackystudio/forge-ssh-assistant",
    "description": "A tool to fetch you Laravel Forge servers to easily SSH into them ",
    "keywords": ["ssh", "laravel forge", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://wackystudio.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/WackyStudio/ForgeSSHAssistant/issues",
        "source": "https://github.com/WackyStudio/ForgeSSHAssistant"
    },
    "authors": [
        {
            "name": "Thomas NÃ¸rgaard",
            "email": "tgn@wackystudio.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel-zero/framework": "5.8.*",
        "nunomaduro/laravel-console-menu": "^2.1",
        "padraic/phar-updater": "^1.0.6",
        "symfony/process": "^4.3",
        "themsaid/forge-sdk": "^2.0",
        "zendframework/zend-text": "^2.7"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["./builds/forge"]
}
