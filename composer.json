{
    "name": "WackyStudio/forge-ssh-assistant",
    "description": "Forge SSH Assistant",
    "keywords": ["laravel", "forge", "ssh", "cli"],
    "homepage": "https://laravel-zero.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/WackyStudio/forge-ssh-assistant/issues",
        "source": "https://github.com/WackyStudio/forge-ssh-assistant"
    },
    "authors": [
        {
            "name": "Thomas NÃ¸rgaard",
            "email": "tgn@wackystudio.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel-zero/framework": "^8.8",
        "nunomaduro/laravel-console-menu": "^3.2.0",
        "symfony/process": "^5.1",
        "laravel/forge-sdk": "^3.6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["forge"]
}
